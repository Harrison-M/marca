{% extends '::base.html.twig' %}
{% block title %}Marca Tags{% endblock %}

{% block stylesheets %}
{% stylesheets 
  'bundles/marcauser/css/*'
  filter='cssrewrite'
%}
<link rel="stylesheet" href="{{ asset_url }}">
{% endstylesheets %}
{% endblock %}


{% block body %}
<div class="row-fluid">
{% block sidebar %}
<div class="span2">
    <div class="well sidebar-nav">
     <ul class="nav nav-list">   
       {% if is_granted('ROLE_USER') %}
             <li><a href="{{ path('tag_new') }}">New Tag</a></li>
             <li><a href="{{ path('tagset_new') }}">New Tagset</a></li>
             <li><a href="{{ path('tag') }}">Edit Tags</a></li>
             <li><a href="{{ path('tagset') }}">Edit Tagsets</a></li>
       {% endif %}
     </ul>
    </div> 
    </div>        
{% endblock %}  
<div class="span10"> 
{% block content %}          
{% endblock %}
</div>   
</div>         
{% endblock %} 