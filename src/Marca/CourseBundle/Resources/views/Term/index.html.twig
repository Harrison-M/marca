{% extends 'MarcaCourseBundle:Term:layout.html.twig' %}

{% block body %}

<div class="row-fluid">
   <div class="span12">
      <a class="btn btn-primary" href="{{ path('term_new') }}">New</a>
   </div>
</div> 

<div class="row-fluid">
<div class="span12">
<table class="table table-bordered">
<tbody>
    {% for entity in entities %}
        <tr>            <td><a href="{{ path('term_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>            <td>{{ entity.term }}</td>            <td>{{ entity.termName }}</td>            <td>{{ entity.status }}</td>            <td><div class="span12">     <a class="btn" href="{{ path('term_show', { 'id': entity.id }) }}">show</a>     <a class="btn" href="{{ path('term_edit', { 'id': entity.id }) }}">edit</a></div>  
            </td>        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}
