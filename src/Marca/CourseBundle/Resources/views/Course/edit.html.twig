
{% extends 'MarcaCourseBundle:Course:layout.html.twig' %}

{% block content %}
<div class="span6 well">
<div class="row-fluid">    
<form action="{{ path('course_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }} novalidate="novalidate">
    {{ form_widget(edit_form) }}
    <div>
        <button class="btn btn-primary" type="submit">Update</button>
    </div>
</form>
</div>
</div>
{% endblock %}
