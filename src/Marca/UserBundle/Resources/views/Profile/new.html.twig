
{% extends 'MarcaUserBundle:Profile:layout.html.twig' %}

{% block content %}
<div class="row-fluid">
<div class="span12">
        <a class="btn" href="{{ path('user_profile') }}">List</a>
</div>

<div class="span12" style="margin-top: 3em"> 
<form action="{{ path('user_profile_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <div>
        <button class="btn" type="submit">Create</button>
    </div>
</form>
</div>
</div>
{% endblock %}
